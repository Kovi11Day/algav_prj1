package com.algav.patricia;

import com.algav.patricia.string.IPATString;

public class PATCase implements IPATCase{
	
	private IPATString word;
	private IPatriciaTrie fils;
	
	public PATCase(IPATString mot, IPatriciaTrie fils){
		this.word = mot;
		this.fils = fils;
	}
	
	public PATCase(IPATString mot){
		this.word = mot;
		this.fils = null;
	}
	
	public IPATString getMot() {
		return word;
	}
	public void setMot(IPATString mot) {
		this.word = mot;
	}
	public IPatriciaTrie getFils() {
		return fils;
	}
	public void setFils(IPatriciaTrie fils) {
		this.fils = fils;
	}
	
	

}

package com.algav;

import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;

import com.algav.patricia.IPatriciaTrie;
import com.algav.patricia.PatriciaTrie;

public class ExtractFiles {
	
	public IPatriciaTrie constructPATtrie(String filename) throws FileNotFoundException{
		File oeuvre = new File (filename);
		Scanner scanner = new Scanner(oeuvre).useDelimiter("\n");
		IPatriciaTrie patrica = new PatriciaTrie();
		//String inputWord;
		while(scanner.hasNext()){
			patrica.ajout(scanner.next());
		}
		return patricia;
	}
}
